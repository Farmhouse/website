<h1>Listing photos</h1>

<table>
  <tr>
    <th>Conf</th>
    <th>Talk</th>
    <th>Caption</th>
    <th>Photographer</th>
    <th>Url</th>
    <th>Canonical url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.conf_id %></td>
    <td><%= photo.talk_id %></td>
    <td><%= photo.caption %></td>
    <td><%= photo.photographer %></td>
    <td><%= photo.url %></td>
    <td><%= photo.canonical_url %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
